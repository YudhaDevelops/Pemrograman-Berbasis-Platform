{
	"php":
	{
		"prefix": "php",
		"body": [
			"<?php $1 ?>"
		],
		"description": "php tag biasa"
	},

	"phpp":
	{
		"prefix": "phpp",
		"body": [
			"<?= $$1; ?>"
		],
		"description": "php tag phpp"
	},

	"group_laravel":
	{
		"prefix": "group-prefix",
		"body": [
			"Route::group([",
				"'controller' => $1::class,",
				"'prefix'    => 'prefixName',",
			"],function(){",
			"",
			"});"
		],
	},
	"routeInFrontEnd":{
		"prefix": "ro",
		"body": [
			"{{route('$1')}}"
		]
	},
	"get aset data":{
		"prefix": "as",
		"body": [
			"{{asset('$1')}}"
		]
	},
	"get iset":{
		"prefix": "is",
		"body": [
			"{{isset($1) ? $1 : ''}}"
		]
	},
	"coment laravel":{
		"prefix": "coLara",
		"body": [
			"{{-- $1 --}}"
		]
	},
	"get_route":{
		"prefix": "routeGetBiasa",
		"body": [
			"Route::get('/$1', '$1')->name('prefix.$1');"
		],
	},
	"get_route_show":{
		"prefix": "routeGetShowId",
		"body": [
			"Route::get('/$1/show/{id}', '$1')->name('show.$1');"
		]
	},
	"get_route_delete":{
		"prefix": "routeGetDeleteId",
		"body": [
			"Route::get('/$1/delete/{id}', '$1')->name('delete.$1');"
		]
	},
	"post_route":{
		"prefix": "routePost",
		"body": [
			"Route::post('/$1', '$1')->name('prefix.$1');"
		]
	},
	"put":{
		"prefix": "routePut",
		"body": [
			"Route::put('/$1/{id}', '$1')->name('prefix.$1');"
		]
	},
	"function_construct":{
		"prefix": "fuConst",
		"body": [
			"public function __construct()",
			"{ $1",	
			"}"
		]
	},
	"function":{
		"prefix": "fu",
		"body": [
			"public function $1()",
			"{",
			"}"
		]
	},
	"function_req":{
		"prefix": "fuReq",
		"body": [
			"public function $1(Request $$request)",
			"{",
			"}"
		]
	},
	"function_req_id":{
		"prefix": "fuReqId",
		"body": [
			"public function $1(Request $$request,$$id)",
			"{",
			"}"
		]
	},
	"function_private_get1":{
		"prefix": "fuPriGet1",
		"body": [
			"private function $1($$data)",
			"{",
			"}"
		]
	},
	"function_private_get2":{
		"prefix": "fuPriGet2",
		"body": [
			"private function $1($$data,$$data)",
			"{",
			"}"
		]
	},
}